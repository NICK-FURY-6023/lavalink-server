# ==============================================================================
# Environment Variables Template
# Copy this file to .env and fill in your values
# ==============================================================================

# ==============================
# LAVALINK CONFIGURATION
# ==============================
LAVALINK_PASSWORD=YourSecurePassword123!

# ==============================
# SPOTIFY API
# Get from: https://developer.spotify.com/dashboard
# ==============================
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_COUNTRY=US

# ==============================
# YOUTUBE OAUTH
# Use the oauth-generator.sh script to get this
# ==============================
YOUTUBE_REFRESH_TOKEN=your_youtube_refresh_token

# ==============================
# CIPHER SERVER
# ==============================
CIPHER_PASSWORD=YourCipherPassword

# ==============================
# APPLE MUSIC
# Get from: https://developer.apple.com/account
# ==============================
APPLE_MUSIC_TOKEN=your_apple_music_token
APPLE_COUNTRY=US

# ==============================
# DEEZER
# ==============================
DEEZER_MASTER_KEY=your_deezer_master_key
DEEZER_ARL=your_deezer_arl

# ==============================
# MULTI-IP EXIT NODES (PREMIUM)
# Replace with your actual proxy IPs
# ==============================
# Singapore Nodes
SG_IP_1=103.xxx.xxx.1
SG_IP_2=103.xxx.xxx.2
SG_IP_3=103.xxx.xxx.3

# Germany Nodes
DE_IP_1=185.xxx.xxx.1
DE_IP_2=185.xxx.xxx.2
DE_IP_3=185.xxx.xxx.3

# US-East Nodes
US_IP_1=45.xxx.xxx.1
US_IP_2=45.xxx.xxx.2
US_IP_3=45.xxx.xxx.3

# Backup Nodes
BACKUP_IP_1=103.xxx.xxx.10
BACKUP_IP_2=185.xxx.xxx.10
BACKUP_IP_3=45.xxx.xxx.10

# ==============================
# HAPROXY
# ==============================
HAPROXY_STATS_PASSWORD=admin123

# ==============================
# GRAFANA
# ==============================
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin123

# ==============================
# SENTRY (Error Tracking)
# ==============================
SENTRY_DSN=

# ==============================
# ALERTING WEBHOOKS
# ==============================
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=
